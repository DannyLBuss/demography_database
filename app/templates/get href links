from bs4 import BeautifulSoup, SoupStrainer
from urllib2 import urlopen
from urllib2 import HTTPError
from urllib2 import URLError
import httplib2
import datetime
import random
import re

optionsUrl = 'https://compadredb.wordpress.com/'
optionsPage = urlopen(optionsUrl)
soup = BeautifulSoup(optionsPage, "html5lib")
middle = soup.findAll("h1", attrs={'class': 'entry-title'})
for line in middle.find_all('a'):
    print(line.get('href'))

print middle
print(len(middle))

import urllib2
from bs4 import BeautifulSoup
url = urllib2.urlopen("https://compadredb.wordpress.com/").read()
soup = BeautifulSoup(url, "html5lib")
for line in soup.find_all('a'):
    print(line.get('href'))
